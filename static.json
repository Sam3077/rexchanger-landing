{
  "root": "public/",
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/**.css": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/**.js": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/icons/*.png": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "https_only": true,
  "error_page": "404.html",
  "redirects": {
    "/terms-of-use": {
      "url": "/terms-of-user.pdf",
      "status": 302
    },
    "/privacy-policy": {
      "url": "/privacy-policy.pdf",
      "status": 302
    },
    "/refer/**": {
      "url": "/",
      "status": 302
    }
  }
}
